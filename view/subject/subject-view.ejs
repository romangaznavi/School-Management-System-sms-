<% include ../layout/header %>
<% include ../layout/top-menu %>
<% include ../layout/side-menu %>
<section class="main">
<table class="table table-striped table-bordered">
    <h4>Subject View
        <span style="float: right; padding-bottom: 10px;"><a href="/subjectList" class="btn btn-primary">Back to List</a></span>
    </h4>
    <thead>
            <% subjectViewResult.forEach((item) => { %>
                <tbody>
        <tr>
            <th>Subject Name</th> <td><%=item.subjectName%></td> </tr>
            <tr><th>Teacher Name</th> <td><%=item.fullName%></td></tr>
            <tr><th>Teaching Day</th> <td><%=item.subjectTeachingDay%></td></tr>
            <tr><th>Start Time</th>   <td><%=item.startTime%></td></tr>
            <tr><th>End Time</th>     <td><%=item.endTime%></td></tr>
        </tr>
        <% }) %>


        
</table>
</section>