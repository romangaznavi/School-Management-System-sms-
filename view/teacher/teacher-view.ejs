
<% include ../layout/header %>
<% include ../layout/top-menu %>
<% include ../layout/side-menu %>
        <section class="main">
        <h4>View Assignment
           <span style="float: right;"><a href="/teacherList" class="btn btn-primary">List Teacher</a></span>
        </h4>
        <table class="table table-striped table-bordered">
            <thead>
                    <% viewTeacherdata.forEach((item) => { %>
                <tbody>
                <tr>
                    <tr><th>Teacher Name</th> <td><%=item.fullName%></td></tr>
                    <tr><th>Class</th> <td><%=item.className%></td></tr>
                    <tr><th>Date of Birth</th> <td><%=item.dob%></td></tr>
                    <tr><th>Gender</th> <td><%=item.gender%></td></tr>
                    <tr><th>Joinig Date</th> <td><%=item.joiningDate%></td></tr>
                    <% }) %>
                </tr>
                </tbody>
            </thead>
    </table>
</section>

<% include ../layout/footer %>