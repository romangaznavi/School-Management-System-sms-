
<% include ../layout/header %>
<% include ../layout/top-menu %>
<% include ../layout/side-menu %>
        <section class="main">
        <h4>View Assignment
           <span style="float: right;"><a href="/assignmentList" class="btn btn-primary">List Assignment</a></span>
        </h4>
        <table class="table table-striped table-bordered">
            <thead>
                <tbody>
                <tr>
                    <tr><th>Student</th> <td><%=viewAssignmentData.name%></td></tr>
                    <tr><th>Subject</th> <td><%=viewAssignmentData.subjectName%></td></tr>
                    <tr><th>Teacher</th> <td><%=viewAssignmentData.fullName%></td></tr>
                    <tr><th>Completed</th> <td><%=viewAssignmentData.completed%></td></tr>
                    <tr><th>Incomplete</th> <td><%=viewAssignmentData.incomplete%></td></tr>
                    <tr><th>Notes</th> <td><%=viewAssignmentData.notes%></td></tr>
                </tr>
                </tbody>
            </thead>
    </table>
</section>

